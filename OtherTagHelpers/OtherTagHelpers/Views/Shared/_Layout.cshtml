<!DOCTYPE html>
<html>
<head>

    <title>OtherTagHelpers</title>
    @*<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />*@
    <link asp-href-include="/lib/bootstrap/dist/**/*.css" asp-href-exclude="/lib/bootstrap/dist/**/*.min.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" asp-fallback-href-include="/lib/bootstrap/dist/**/*.min.css" asp-fallback-test-class="btn" asp-fallback-test-property="display" asp-fallback-test-value="inline-block" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <br /><br />
        <environment names="development">
            <div class="panel bg-info">Development</div>
        </environment>

        <environment names="production">
            <div class="panel bg-danger">Production</div>
        </environment>
        @RenderBody()
    </div>
    @*<script asp-src-include="/lib/jquery/dist/**/*.js"></script>*@
    @*<script asp-src-include="/lib/jquery/dist/**/*.js" asp-src-exclude="**.min.js"></script>*@
    @*<script asp-src-include="/lib/jquery/dist/**/*.js" asp-src-exclude="**.min.js" asp-append-version="true"></script>*@
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" asp-fallback-src-include="/lib/jquery/dist/**/*.min.js" asp-fallback-test="window.jQuery"></script>

</body>
</html>
